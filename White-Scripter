local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("BASKETBALL RNG", "DarkTheme")
local Tab = Window:NewTab("SCRIPT")
local Section = Tab:NewSection("SPIN CARD")
Section:NewButton("Auto Spin Cardüñ±Ô∏è", "ButtonInfo", function()
while true do 
wait(0.0)
local args = {
    [1] = "Default"
}

game:GetService("ReplicatedStorage"):WaitForChild("Knit"):WaitForChild("Services"):WaitForChild("GachaService"):WaitForChild("RF"):WaitForChild("AttemptToSpinGacha"):InvokeServer(unpack(args))
end
    print("Clicked")
end)
local Tab = Window:NewTab("DELETE CARD")
